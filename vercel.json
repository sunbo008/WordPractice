{
  "version": 2,
  "name": "word-tetris-game",
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "proj",
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/settings.html",
      "dest": "/public/settings.html"
    },
    {
      "src": "/test-structure.html",
      "dest": "/public/test-structure.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/words/(.*)",
      "dest": "/public/words/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
