{
  "version": 2,
  "name": "word-tetris-game",
  "builds": [
    {
      "src": "proj/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/proj/public/index.html"
    },
    {
      "src": "/settings.html",
      "dest": "/proj/public/settings.html"
    },
    {
      "src": "/test-structure.html",
      "dest": "/proj/public/test-structure.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/proj/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/proj/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/proj/public/images/$1"
    },
    {
      "src": "/words/(.*)",
      "dest": "/proj/public/words/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/proj/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/proj/public/$1"
    }
  ]
}
